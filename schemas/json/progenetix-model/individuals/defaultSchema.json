{
    "$id": "https://progenetix.org/services/schemas/individual/v2021-11-12",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": true,
    "properties": {
        "ancestries": {
            "items": {
                "$ref": "../common/ancestry.json#/properties"
            },
            "type": "array"
        },
        "dataUseConditions": {
            "$ref": "../common/ontologyTerm.json#/properties",
            "examples": [
                {
                    "id": "DUO:0000004",
                    "label": "no restriction"
                }
            ]
        },
        "dateOfBirth": {
            "examples": [
                "1967-11-11",
                "2002-09-21"
            ],
            "format": "date-time",
            "type": "string"
        },
        "description": {
            "examples": [
                "HapMap project contributor"
            ],
            "type": "string"
        },
        "diseases": {
            "items": {
                "$ref": "../common/disease.json#/properties"
            },
            "type": "array"
        },
        "ethnicity": {
            "$ref": "../common/commonDefinitions.json#/definitions/Ethnicity"
        },
        "exposures": {
            "items": {
                "$ref": "../common/exposure.json#/properties"
            },
            "type": "array"
        },
        "externalReferences": {
            "examples": [
                {
                    "description": "HapMap individual",
                    "id": "biosample:SAME122868"
                },
                {
                    "id": "PMID:9950502"
                },
                {
                    "description": "Prognostic analysis of mantle cell lymphoma genomes",
                    "id": "geo:GSE13331"
                }
            ],
            "items": {
                "$ref": "../common/externalReference.json#/properties"
            },
            "type": "array"
        },
        "geographicOrigin": {
            "$ref": "../common/commonDefinitions.json#/definitions/GeographicLocation"
        },
        "id": {
            "description": "The resource-local identifier for this individual.",
            "examples": [
                "pgxind-kftx25eh"
            ],
            "type": "string"
        },
        "info": {
            "description": "Additional structured data which may not be common across different Beacon instances.",
            "type": "object"
        },
        "interventionsOrProcedures": {
            "items": {
                "$ref": "../common/procedure.json#/properties"
            },
            "type": "array"
        },
        "karyotypicSex": {
            "description": "Chromosomal sex of an individual as being used by Phenopackets. Following the definition there",
            "enum": [
                "UNKNOWN_KARYOTYPE",
                "XX",
                "XY",
                "XO",
                "XXY",
                "XXX",
                "XXYY",
                "XXXY",
                "XXXX",
                "XYY",
                "OTHER_KARYOTYPE"
            ],
            "type": "string"
        },
        "measures": {
            "items": {
                "$ref": "../common/measurement.json#/properties"
            },
            "type": "array"
        },
        "pedigrees": {
            "items": {
                "$ref": "../common/pedigree.json#/properties"
            },
            "type": "array"
        },
        "phenotypicFeatures": {
            "items": {
                "$ref": "../common/phenotypicFeature.json#/properties"
            },
            "type": "array"
        },
        "provenance": {
            "$ref": "../common/provenance.json#/properties"
        },
        "sex": {
            "$ref": "../common/commonDefinitions.json#/definitions/Sex"
        },
        "treatments": {
            "items": {
                "$ref": "../common/treatment.json#/properties"
            },
            "type": "array"
        },
        "updated": {
            "examples": [
                "2020-09-10T17:44:10.170Z"
            ],
            "format": "date-time",
            "type": "string"
        },
        "vitalStatus": {
            "$ref": "../common/vitalStatus.json#/properties"
        }
    },
    "required": [
        "id",
        "sex"
    ],
    "title": "Individual",
    "type": "object"
}